{
    "kind": "collectionType",
    "collectionName": "tasks",
    "info": {
        "singularName": "task",
        "pluralName": "tasks",
        "displayName": "Task",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "title": {
            "type": "string"
        },
        "description": {
            "type": "text"
        },
        "startDate": {
            "type": "datetime"
        },
        "closeDate": {
            "type": "datetime"
        },
        "dueDate": {
            "type": "datetime"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "not started",
                "in progress",
                "completed"
            ]
        },
        "userProfile": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::user-profile.user-profile"
        },
        "tags": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::tag.tag"
        },
        "agentRouter": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::agent-router.agent-router"
        }
    }
}
